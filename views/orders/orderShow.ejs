<table class="table">

  <% if (curr_user) { %>
    <% for (var i=0; i<orderHistory.length; i++) { %>
     <% var oldDate = orderHistory[i].created_at; %>
        <% var oldDateString = oldDate.toString().split(' '); %>
        <% var newDate = oldDateString[1]+"/"+oldDateString[2]+"/"+oldDateString[3]; %>


        <% var desserts = orderHistory[i].dessert_items %>
        <% var dessertsList = "" %>
      <%  for (var j=0; j < desserts.length; j++) { %>
            <% dessertsList += desserts[j]._id + ", " %>

       <%  } %>

    <tr>
      <td>order <%= i + 1 %></td>
      <td>Date: <%= newDate %></td>
      <td>Items: <%= dessertsList.slice(0, dessertsList.length - 2); %> </td>
    </tr>
    <% } %>
</table>
